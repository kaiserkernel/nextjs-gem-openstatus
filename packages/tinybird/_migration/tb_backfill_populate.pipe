NODE mat_node
SQL >

    SELECT
        latency,
        monitorId,
        toLowCardinality(region) region,
        statusCode,
        timestamp,
        url,
        workspaceId,
        cronTimestamp,
        message
    FROM ping_response__v5
    WHERE fromUnixTimestamp64Milli(cronTimestamp) <= '2024-01-15 18:16:00.000'

TYPE materialized
DATASOURCE ping_response__v6